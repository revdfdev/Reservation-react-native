    "apollo-client": "^2.4.13",
    "graphql": "^14.1.1",
    "moment": "^2.24.0",
    "native-base": "2.11.0",
    "prop-types": "15.6.1",
    "react": "^16.8.2",
    "react-apollo": "^2.4.1",
    "react-native": "^0.58.5",
    "react-native-config": "^0.11.7",
    "react-native-elements": "^1.0.0",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-vector-icons": "6.3.0",
